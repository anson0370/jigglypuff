{
  "name": "jigglypuff",
  "description": "Fake Aixforce web container",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "express": "~3",
    "handlebars": "~1.3.0",
    "lodash": "~2.4.1",
    "async": "~0.2.9",
    "tracer": "~0.6.1",
    "redis": "~0.10.0"
  },
  "devDependencies": {
    "should": "~2.1.1",
    "coffee-script": "~1.6.3",
    "mocha": "~1.17.0"
  },
  "bin": {
    "jiggly": "./bin/jiggly"
  },
  "scripts": {
    "compile": "rm -rf ./lib && coffee --compile --output lib/ src/",
    "build": "npm run compile && npm i -g .",
    "test": "cd test && env NODE_ENV=test mocha **/*.coffee --compilers coffee:coffee-script --require should -R spec",
    "start": "coffee ./src/index.coffee"
  }
}
